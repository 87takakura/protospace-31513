<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <span>こんにちは、</span>
        <%= link_to "#{current_user.name}", user_path(current_user), class: :greeting__link %>
        <span>さん</span>
      </div> 
    <% end %>
    <div class="card__wrapper">
     <div class = "prototype" >
       <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
      <%= render partial: 'prototype', collection: @prototypes %>
     </div>
    </div>
  </div>
</main>
